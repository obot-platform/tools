Name: GitLab Credential
Share Credential: gitlab-cred as gitlabToken
Type: credential

---
Name: gitlab-cred
Tools: ../../generic-credential

#!sys.call ../../generic-credential

{
	"promptInfo": {
		"fields" : [
			{
                "name": "GitLab token",
                "description": "Your GitLab token",
                "env": "GITLAB_TOKEN",
                "sensitive": true
            }
		],
		"message": "Please enter your GitLab token."
	}
}
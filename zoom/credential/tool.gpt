Name: Zoom OAuth Credential
Share Credential: ../../oauth2 as zoom
    with ZOOM_OAUTH_TOKEN as token and
        zoom as integration and
        user:read:user
        meeting:read:list_templates
        meeting:write:meeting
        meeting:read:list_meetings
        meeting:read:meeting
        meeting:delete:meeting
        meeting:update:meeting:admin
        " as scope
Type: credential
